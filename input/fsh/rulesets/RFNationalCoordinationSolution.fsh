RuleSet: CaseNumber(case-id)
// sets up a logical Identifier to a salesforce rheumatic fever case 

* use = #usual
* system = "https://standards.digital.health.nz/ns/rheumatic-fever-identifiers"
* value = "{case-id}"

RuleSet: ConditionNumber(condition-id)
// sets up a logical Identifier to a salesforce condition object

* use = #usual
* system = "https://standards.digital.health.nz/ns/rheumatic-fever-identifiers"
* value = "{condition-id}"